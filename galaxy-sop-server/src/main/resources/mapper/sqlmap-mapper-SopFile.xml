	<select id="selectPageFile" resultMap="BaseResultMap" parameterType="java.util.Map">
			select 	
			<include refid="Page_Column_List" />		
			from sop_file sf
					left join sop_voucher_file svf on sf.voucher_id = svf.id
					left join platform_user pu on sf.file_uid = pu.id,
					sop_project sp
			where	sf.project_id = sp.id
			and		(select dict.dict_value  from dict where dict.dict_code = sp.project_progress) >= (select dict.dict_value  from dict where dict.dict_code = sf.project_progress)
			and		sf.file_worktype is not null and sf.file_worktype != 'fileWorktype:12'
			<![CDATA[ and		case when sp.stock_transfer <> '1' then sf.file_worktype != 'fileWorktype:7' else 1=1 end ]]>
			<include refid="Page_Where_Clause" />
			<if test="sorting != null">order by ${sorting}</if>
			<if test="offset != null and limit != null">
				limit #{offset}, #{limit}
			</if>			
	</select>
	
	
